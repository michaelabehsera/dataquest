mixin header
	header.header#header
		.header_row#header_row
			.header_row_in
				a(href="#").logo
					h1.logo_text
						+logo_text_svg
						.logo_anim_block
		.head_menu_trigger
			.user_menu_butt#user_menu_butt
				span.user_menu_butt_decor
		.head_menu_block#head_menu_block
			nav.head_menu_in
				+main_menu(headerMenuArray)

mixin footer
	footer.footer
		.footer_in
			.footer_logo_w
				a(href="#").footer_logo dataquest
			.footer_menu_block
				h2.footer_title Menu
				ul.footer_menu_list
					li.footer_menu_item
						a(href="#").footer_menu_link About
					li.footer_menu_item
						a(href="#").footer_menu_link Contact
					li.footer_menu_item
						a(href="#").footer_menu_link Help
					li.footer_menu_item
						a(href="#").footer_menu_link Blog
					li.footer_menu_item
						a(href="#").footer_menu_link Subscribe

			.footer_soc_block
				h2.footer_title Follow Us
				ul.footer_soc_list
					li.footer_soc_item
						a(href="#").footer_soc_link Twitter
					li.footer_soc_item
						a(href="#").footer_soc_link Facebook
					li.footer_soc_item
						a(href="#").footer_soc_link Linkedin

mixin course_item(title,text,icon_mod)
	li.course_item
		a(href="#").course_block
			dl.course_dl
				dt.course_title #{title}
				dd.course_def
					p #{text}
			.course_icon(class="#{icon_mod}_mod")
				+box_icon
			.course_link_decor
				+right_arrow_icon

mixin track_item(mod,title)
	li.track_item(class="#{mod}")
		article.track_block
			.track_block_main
				a(href="#").track_add_butt
				h3.track_title #{title}
				.track_text
					p.
						Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod
						incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam
						nostrud exercitation ullamco laboris nisi ut aliquip ex ea commodo
			footer.track_footer
				ul.track_info_list
					li.track_info_item
						.track_info_text
							span 5 Steps
							img(alt="" src="i/layers_white.png").track_info_icon.step_mod
					li.track_info_item
						.track_info_text
							span 100 Students
							img(alt="" src="i/reading_small.png").track_info_icon.stud_mod

mixin product_feat_item(title,icon)
	li.product_feat_item
		.product_feat_item_block
			h4.product_feat_title #{title}
			p.product_feat_text Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. Ut enim ad minim veniam,
			if icon === 'box'
				+box_icon('product_icon gray_mod')
			if icon === 'chart'
				+chart_icon('product_icon gray_mod')
			if icon === 'palette'
				img(alt="" src="i/palette_gray.png").palette_icon.product_icon
			if icon === 'pen'
				img(alt="" src="i/pen_gray.png").pen_icon.product_icon
			if icon === 'shield'
				img(alt="" src="i/shield_check_gray.png").shield_icon.product_icon
			if icon === 'heart'
				+heart_icon('product_icon gray_mod')





